{
  "dashboard": {
    "title": "Flores Victoria - E-Commerce Performance",
    "tags": ["e-commerce", "performance", "business"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Web Vitals - FCP & LCP",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "Median"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [2], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Response Time Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Rate (req/s)",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m])) by (method, status)",
            "legendFormat": "{{method}} - {{status}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8},
        "type": "stat",
        "title": "Active Users (Real-time)",
        "targets": [
          {
            "expr": "sum(active_users)",
            "legendFormat": "Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8},
        "type": "stat",
        "title": "Conversion Rate (%)",
        "targets": [
          {
            "expr": "(sum(rate(orders_completed_total[1h])) / sum(rate(cart_views_total[1h]))) * 100",
            "legendFormat": "Conversion"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 2, "color": "yellow"},
                {"value": 5, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
        "type": "stat",
        "title": "Cart Abandonment (%)",
        "targets": [
          {
            "expr": "(1 - (sum(rate(orders_completed_total[1h])) / sum(rate(cart_created_total[1h])))) * 100",
            "legendFormat": "Abandonment"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 70, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8},
        "type": "stat",
        "title": "Average Order Value",
        "targets": [
          {
            "expr": "avg(order_total_value)",
            "legendFormat": "AOV"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "MongoDB Query Performance",
        "targets": [
          {
            "expr": "rate(mongodb_op_latencies_ops_total[5m])",
            "legendFormat": "{{type}} operations"
          },
          {
            "expr": "mongodb_connections{state=\"current\"}",
            "legendFormat": "Connections"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "System Resources (CPU & Memory)",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yaxes": [
          {"format": "percent", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (service)",
            "legendFormat": "{{service}} errors"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "Network I/O",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total[5m])",
            "legendFormat": "Receive"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total[5m])",
            "legendFormat": "Transmit"
          }
        ],
        "yaxes": [
          {"format": "Bps", "label": "Bytes/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "type": "table",
        "title": "Top Products (by views)",
        "targets": [
          {
            "expr": "topk(10, sum(product_views_total) by (product_id, product_name))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "product_id": "Product ID",
                "product_name": "Product Name",
                "Value": "Views"
              }
            }
          }
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "type": "piechart",
        "title": "Traffic Sources",
        "targets": [
          {
            "expr": "sum(page_views_total) by (source)",
            "legendFormat": "{{source}}"
          }
        ]
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "(sum(rate(cache_hits_total[5m])) / sum(rate(cache_requests_total[5m]))) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "yaxes": [
          {"format": "percent", "max": 100, "min": 0},
          {"format": "short"}
        ]
      }
    ]
  }
}
