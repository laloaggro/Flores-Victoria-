{
  "plugins": {
    "postcss-import": {},
    "autoprefixer": {
      "browsers": [
        "last 2 versions",
        "> 1%",
        "not dead"
      ]
    },
    "cssnano": {
      "preset": ["default", {
        "discardComments": {
          "removeAll": true
        },
        "normalizeWhitespace": true,
        "colormin": true,
        "minifyFontValues": true,
        "minifyGradients": true
      }]
    }
  }
}
