{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Product service for Flores Victoria e-commerce",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "axios": "^1.4.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.8",
    "prom-client": "^15.1.0",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "@types/jest": "^29.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testMatch": ["**/__tests__/**/*.test.js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/**/__tests__/**"
    ]
  },
  "directories": {
    "logs": "logs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
